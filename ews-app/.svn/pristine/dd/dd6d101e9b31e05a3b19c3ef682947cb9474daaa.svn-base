.modalstyle{
    max-height: 450px;
}

.workmodalstyle{
    max-height: 700px;
}

.ant-tabs-bar{
    margin: 0px;
}
.carimage{
    width: 50%;
}
/*工单详情页面结束*/
.dynamic-delete-button {
    cursor: pointer;
    position: relative;
    top: 4px;
    font-size: 24px;
    color: #999;
    transition: all 0.3s;
}
.dynamic-delete-button:hover {
    color: #777;
}
.dynamic-delete-button[disabled] {
    cursor: not-allowed;
    opacity: 0.5;
}
