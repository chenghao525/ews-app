body {
  margin: 0;
  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", "Roboto", "Oxygen",
    "Ubuntu", "Cantarell", "Fira Sans", "Droid Sans", "Helvetica Neue",
    sans-serif;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  font-size:13px;
}

code {
  font-family: source-code-pro, Menlo, Monaco, Consolas, "Courier New",
    monospace;
}

.loginBox {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: flex-start;
  flex-flow: column;
}
.loginInfo {
  margin-top: 13%;
  margin-left: 10%;
  width: 320px;
  height: 300px;
}
.loginHeader {
  height: 11%;
  padding: 20px 0 20px 10%;
}
.loginHeader img {
  height: 100%;
}
.loginMiddle {
  height: 80%;
  background: url("../assets/ews-bg.png") no-repeat;
}

.header-box {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.logo {
  width: 121px;
  height: 25px;
}
.loginFooter {
  margin-top: 15px;
  text-align: center;
  line-height: 20px;
}

.ant-form-item{
  margin-bottom: 0px !important;
}

.ant-card-head{
  font-size: 14px !important;
  /*background: #CCE8EB !important;
  color:  #4cabce !important;*/
  text-align: center;
  font-weight: bold;
}





